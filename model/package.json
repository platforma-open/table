{
  "name": "@milaboratory/milaboratories.table.model",
  "version": "0.2.3",
  "description": "Block model",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup && vite build && block-tools build-model"
  },
  "dependencies": {
    "@milaboratory/sdk-ui": "catalog:",
    "@milaboratory/sdk-vue": "catalog:"
  },
  "devDependencies": {
    "@milaboratory/pl-block-tools": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "treeshake": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": false,
    "sourcemap": true
  }
}
